pipeline {
   agent any
   tools {nodejs "node"}
 

   stages {
      stage('Cloning Git') {
         steps {
            git 'https://github.com/CrisAlva25/ADYS2_ProyectoClase'
         }
      }
         
      stage('Install dependencies') {
         steps {
            sh 'npm install'
         }
      }
      
      stage('Test') {
         steps {
            echo 'Testing...'
         }
      }
      stage('Deploy') {
         steps {
            echo 'Deploying...'
         }
      }
   }
}
